{
	"env": {
			"browser": true,
			"es2021": true
	},
	"extends": [
			"airbnb-base",
			"prettier"
	],
	"overrides": [
	],
	"parserOptions": {
			"ecmaVersion": "latest",
			"sourceType": "module"
	},
	"rules": {
			"no-console": 0,
			"no-unused-vars": 0,
			"no-param-reassign": 0,
			"no-use-before-define": 0
	}
}
